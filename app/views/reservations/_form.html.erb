<div class="text-field">
  <%= form.label :start_date %>
  <%= form.text_field :start_date, id: "start_date" %>
</div>

<div class="text-field">
  <%= form.label :end_date %>
  <%= form.text_field :end_date, id: "end_date" %>
</div>

<div class="text-field">
  <%= form.label :pax %>
  <%= form.text_field :pax %>
</div>

<script>
  $( "#start_date" ).datepicker();
  $( "#end_date" ).datepicker();
</script>
