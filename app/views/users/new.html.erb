<div id="clearance" class="sign-up">
  <%= link_to "Sign in with Facebook", "/auth/facebook" %>

  <h2><%= t(".title") %></h2>

  <%= form_for @user do |form| %>
    <%= render partial: '/users/form', object: form %>

    <div class="submit-field">
      <%= form.submit %>
    </div>

  <% end %>
</div>
